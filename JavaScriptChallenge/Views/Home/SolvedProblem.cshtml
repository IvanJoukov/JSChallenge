
<div>
    <section>
        @Html.Partial("_Editor", this.ViewData)
        <article id="problemDescription">
            @ViewBag.ProblemDescription
        </article>
    </section>
    <span class="correct-answer">Congratulations! You got the right answer, @ViewBag.CorrectAnswer.  Click the link to pick the next @Html.ActionLink("Problem", "Index", "Home"). </span>
</div>
<script src="http://cdnjs.cloudflare.com/ajax/libs/ace/1.1.3/ace.js" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript">
    @Html.Raw(ViewBag.SetupJavaScriptCode)

    debugger;
    $(document).ready(function () {
        $(".correct-answer").show();
    });

    var editor = ace.edit("editor");
    editor.setTheme("ace/theme/monokai")
    editor.getSession().setMode("ace/mode/javascript");

</script>
